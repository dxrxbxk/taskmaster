[]
cmd = "/usr/sbin/nginx" # command to run
numprocs = 1 # number of processes
umask = 022 # file mode creation mask
workingdir = /tmp # working directory
autostart = true # start at launch
autorestart = unexpected # restart when unexpected exit
exitcodes = [0, 2]
startretries = 3
starttime = 5
stopsignal = TERM
stoptime = 10
stdout = /tmp/nginx.stdout
stderr = /tmp/nginx.stderr
env = [STARTED_BY='taskmaster', PATH='/bin:/usr/bin:/sbin']

[nginx]
